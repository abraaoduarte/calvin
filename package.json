    "morgan": "^1.10.0",
    "pg": "^8.6.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.32",
    "typeorm-pagination": "^2.0.3",
    "typescript": "^4.2.4",
    "winston": "^3.3.3",
    "yarn": "^1.22.10",
    "yup": "^0.32.9",
